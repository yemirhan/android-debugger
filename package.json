{
  "name": "android-debugger",
  "version": "1.0.0",
  "private": true,
  "description": "React Native Android debugging tool",
  "scripts": {
    "dev": "turbo run dev",
    "dev:example": "pnpm --filter @android-debugger/example-expo dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "publish:packages": "pnpm build && pnpm --filter @yemirhan/android-debugger-sdk publish --access public && pnpm --filter @yemirhan/android-debugger-ui publish --access public"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">=18.0.0"
  }
}
